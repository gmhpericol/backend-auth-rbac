	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"const accessToken = pm.collectionVariables.get(\"accessToken\");\r",
					"const expiresAt = pm.collectionVariables.get(\"expiresAt\");\r",
					"\r",
					"const now = Date.now();\r",
					"\r",
					"// dacă nu există token sau e expirat\r",
					"if (!accessToken || !expiresAt || now >= expiresAt) {\r",
					"    pm.sendRequest({\r",
					"        url: pm.environment.get(\"baseUrl\") + \"/auth/login\",\r",
					"        method: \"POST\",\r",
					"        header: {\r",
					"            \"Content-Type\": \"application/json\"\r",
					"        },\r",
					"        body: {\r",
					"            mode: \"raw\",\r",
					"            raw: JSON.stringify({\r",
					"                email: pm.environment.get(\"email\"),\r",
					"                password: pm.environment.get(\"password\")\r",
					"            })\r",
					"        }\r",
					"    }, function (err, res) {\r",
					"        if (err) {\r",
					"            console.error(\"Auth error\", err);\r",
					"            return;\r",
					"        }\r",
					"\r",
					"        const json = res.json();\r",
					"        pm.collectionVariables.set(\"accessToken\", json.access_token);\r",
					"        pm.collectionVariables.set(\"expiresAt\", Date.now() + json.expires_in * 1000);\r",
					"    });\r",
					"}\r",
					""
				]
			}
		},
